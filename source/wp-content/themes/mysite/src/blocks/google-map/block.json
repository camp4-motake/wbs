{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wbs/google-map",
	"version": "0.1.0",
	"title": "Google Map",
	"category": "wbs-blocks",
	"icon": "location",
	"description": "display Google Map and other iFrame content box Block",
	"example": {},
	"supports": {
		"html": false,
		"align": true,
		"alignWide": true,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"mapSource": {
			"type": "string",
			"default": ""
		},
		"aspectRatio": {
			"type": "string",
			"default": "4/3"
		}
	},
	"textdomain": "wbs",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
