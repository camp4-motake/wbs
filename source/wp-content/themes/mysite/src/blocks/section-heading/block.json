	"styles": [
		{ "name": "obj-rd", "label": "赤", "isDefault": true },
		{ "name": "obj-or", "label": "橙" },
		{ "name": "obj-yl", "label": "黄" },
		{ "name": "obj-gr", "label": "緑" },
		{ "name": "obj-bl", "label": "青" },
		{ "name": "obj-pl", "label": "紫" }
	],
	"textdomain": "wbs",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
