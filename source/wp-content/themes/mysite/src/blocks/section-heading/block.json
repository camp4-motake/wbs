{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wbs/section-heading",
	"version": "0.1.0",
	"title": "セクション見出し",
	"category": "wbs-theme-blocks",
	"icon": "heading",
	"description": "",
	"attributes": {
		"level": {
			"type": "number",
			"default": 2
		},
		"alignment": {
			"type": "string",
			"default": "center"
		},
		"headContent": {
			"type": "string",
			"default": ""
		},
		"content": {
			"type": "string",
			"default": ""
		}
	},
	"example": {},
	"supports": {
		"align": false,
		"anchor": true,
		"ariaLabel": true,
		"className": true,
		"html": false,
		"color": {
			"text": false,
			"background": false
		},
		"spacing": {
			"margin": [ "top", "bottom" ],
			"padding": false,
			"__experimentalDefaultControls": {
				"margin": false,
				"padding": false
			}
		}
