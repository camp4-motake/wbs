{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wbs/tab",
	"version": "0.1.0",
	"title": "Tab",
	"category": "wbs-blocks",
	"icon": "welcome-add-page",
	"description": "Tab Content Block",
	"parent": ["wbs/tabs"],
	"supports": {
		"html": false
	},
	"keywords": ["tab"],
	"attributes": {
		"tabLabel": {
			"type": "string",
			"default": ""
		},
		"blockIndex": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "wbs",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
