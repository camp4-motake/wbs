{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wbs/category-loop",
	"version": "0.1.0",
	"title": "カテゴリーループ",
	"category": "wbs-theme-blocks",
	"icon": "excerpt-view",
	"description": "",
	"example": {},
	"supports": {
		"html": false,
		"inserter": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"postType": {
			"type": "string",
			"default": "area"
		},
		"category": {
			"type": "string",
			"default": "category-area"
		},
		"displayMode": {
			"type": "string",
			"default": "article"
		}
	},
	"textdomain": "dynamic",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
