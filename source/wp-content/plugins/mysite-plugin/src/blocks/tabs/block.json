{
	"apiVersion": 2,
	"name": "wbs/tabs",
	"version": "0.1.0",
	"title": "Tab Layout",
	"category": "design",
	"icon": "index-card",
	"description": "Display content in a tabbed layout",
	"supports": {
		"html": false
	},
	"keywords": [
		"tab",
		"tabbed content"
	],
	"attributes": {
		"tabLabelsArray": {
			"type": "array",
			"default": []
		},
		"updateChild": {
			"type": "boolean",
			"default": false
		},
		"sideTabLayout": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "wbs",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
