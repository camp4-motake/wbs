{
	"name": "wbs-theme",
	"description": "Site Block theme",
	"version": "0.1.0",
	"author": "CAMP4 inc.",
	"license": "GPL-2.0-or-later",
	"private": true,
	"scripts": {
		"dev": "wp-scripts start --experimental-modules --webpack-copy-php",
		"build": "wp-scripts build --experimental-modules --webpack-copy-php",
		"lint": "wp-scripts lint-js && wp-scripts lint-style",
		"format": "wp-scripts lint-style --fix && wp-scripts lint-js --fix && wp-scripts format",
		"packages-update": "wp-scripts packages-update",
		"prebuild": "rimraf build/*",
		"predev": "rimraf build/*"
	},
	"dependencies": {
		"@acab/reset.css": "0.11.0",
		"@floating-ui/dom": "1.7.1",
		"@wordpress/icons": "10.25.0",
		"@wordpress/interactivity": "6.25.0",
		"@wordpress/scripts": "30.18.0",
		"clsx": "2.1.1",
		"copy-webpack-plugin": "13.0.0",
		"cssnano": "7.0.7",
		"dialog-closedby-polyfill": "1.1.0",
		"image-minimizer-webpack-plugin": "4.1.3",
		"invokers-polyfill": "0.5.5",
		"masonry-pf": "1.1.1",
		"postcss-easy-import": "4.0.0",
		"postcss-preset-env": "10.2.3",
		"postcss-url": "10.1.3",
		"rimraf": "6.0.1",
		"sharp": "0.34.2",
		"svgo": "3.3.2",
		"swiper": "11.2.8"
	},
	"devDependencies": {
		"stylelint-config-recess-order": "7.1.0"
	}
}
